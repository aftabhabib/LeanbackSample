<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!--底层使用蓝色填充色-->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#00ffff" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:end="2dp"
        android:shape="rectangle"
        android:start="2dp"
        android:top="2dp">
        <shape>
            <solid android:color="#1e6996" />
        </shape>
    </item>

</layer-list>
